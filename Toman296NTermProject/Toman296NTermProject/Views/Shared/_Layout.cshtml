<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

    <div class="contentBox">
        <h3>@ViewBag.Message</h3>
        @RenderBody()
        <hr />
        <footer>
            <p>

                <a href="#" class="btn btn-default">
                    <span class="glyphicon glyphicon-cog"></span>&nbsp;&nbsp;data engine <small>...someday with analytics</small>
                </a>
                &nbsp;&nbsp;&nbsp;
                <a href="~/Contact" class="btn btn-default">
                    <span class="glyphicon glyphicon-envelope"></span>&nbsp;&nbsp;contact
                </a>
                &nbsp;&nbsp;&nbsp;
                <a href="#" class="btn btn-default">
                    <span class="glyphicon glyphicon-book"></span>&nbsp;&nbsp;resources
                </a>
                &nbsp;&nbsp;&nbsp;
                <a href="#" class="btn btn-default">
                    <span class="glyphicon glyphicon-list-alt"></span>&nbsp;&nbsp;directory
                </a>
                &nbsp;&nbsp;&nbsp;
                <a href="~/About" class="btn btn-default">
                    <span class="glyphicon glyphicon-star"></span>&nbsp;&nbsp;about
                </a>

            </p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
